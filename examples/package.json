{
  "name": "directus-activity-analytics",
  "version": "1.0.0",
  "description": "Efficient Directus activity analytics with <3s query time for 100k+ records",
  "type": "module",
  "main": "activity-analytics.js",
  "scripts": {
    "start": "node activity-analytics.js",
    "dev": "nodemon activity-analytics.js",
    "setup": "psql -U $POSTGRES_USER -d $POSTGRES_DB -f setup-database.sql",
    "test": "node test-performance.js",
    "warm-cache": "curl -X POST http://localhost:3000/api/cache/warm",
    "clear-cache": "curl -X DELETE http://localhost:3000/api/cache/clear",
    "cache-stats": "curl http://localhost:3000/api/cache/stats"
  },
  "keywords": [
    "directus",
    "analytics",
    "activity",
    "aggregation",
    "performance",
    "postgresql",
    "redis",
    "caching"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@directus/sdk": "^17.0.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
